{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"triple-equals": true,
		"variable-name": [
			true,
			"check-format",
			"allow-leading-underscore"
		],
		"indent": [true, "spaces", 4]
	},
	"defaultSeverity": "warning"
}